     <FormSelect\\r\\n                    className=\\\"ml-1\\\"\\r\\n                    value={classificationId}\\r\\n                    onChange={(event) => setClassificationId(event.target.value)}\\r\\n                    id=\\\"project\\\"\\r\\n                  >\\r\\n                    <option value='todos'>Filtrar por classificao</option>\\r\\n                    {\\r\\n                      classifications.data && classifications.data.healthFacilityClassifications.map((classification, index) => (\\r\\n                        <option value={classification.id}>{classification.name}</option>\\r\\n                      ))\\r\\n                    }\\r\\n                  </FormSelect>\\r\\n                </Col>\\r\\n                \\r\\n                <Col className=\\\"col-lg-3 d-flex justify-content-end\\\">\\r\\n                  {(data && (name || provinceId || districtId || classificationId)) && (\\r\\n                    <div>\\r\\n                      <Button\\r\\n                        size=\\\"sm\\\"\\r\\n                        className=\\\"btn btn-outline-primary mr-2\\\"\\r\\n                        onClick={clearParams}\\r\\n                      >\\r\\n                        {t(\\\"limpar_dados\\\")}\\r\\n                        <i class=\\\"fa fa-refresh\\\" aria-hidden=\\\"true\\\"></i>\\r\\n                      </Button>\\r\\n                    </div>\\r\\n                  )} \\r\\n\\r\\n                  {data && <TableDataExporter fileName='Lista de Unidades Sanitrias - MFL-External' fileTitle='MFL-Lista de Unidades Sanitrias' tableColums={tableColumExport} tableData={tableDataExport} isFetching={isFetchingExport} />}\\r\\n           \\r\\n                </Col>\\r\\n                \\r\\n              </Row>\\r\\n              <Row />\\r\\n            </CardHeader>\\r\\n            {isFetching && <LinearProgress />}\\r\\n            <CardBody className=\\\"p-0 pb-3\\\">\\r\\n              <Table isFetching={isFetching} error={error} tableColumns={tableColums} tableData={tableData} />\\r\\n              <div className=\\\"d-none\\\">\\r\\n                <TableToExport isFetching={isFetching} error={error} tableColumns={tableColumExport} tableData={tableDataExport} />\\r\\n                \\r\\n              </div>\\r\\n            </CardBody>\\r\\n            {isFetching === false && <TablePaginationDemo totalPages={data} setCurrentPage={setCurrentPage} currentPage={currentPage} rowsPerPage={rowsPerPage} setRowsPerPage={setRowsPerPage} />}\\r\\n          </Card>\\r\\n        </Col>\\r\\n      </Row>\\r\\n    </Container>\\r\\n  );\\r\\n};\\r\\n\\r\\nexport default OrgUnits;\\r\\n\",\"import React from \\\"react\\\";\\r\\nimport { Container, Row, Col, Card } from \\\"shards-react\\\";\\r\\nimport \\\"../../assets/styles/apidocs.css\\\";\\r\\n\\r\\nconst APIDocumentation = () => {\\r\\n\\r\\n  var string = \\\"Alzira de Francisco de Xerinda\\\";\\r\\n  var names = string.split(\\\" \\\");\\r\\n  //names = names.filter(e => e !== 'de')\\r\\n\\r\\n/*   var initials = names[0].substring(0, 1).toUpperCase() + \\\". \\\";\\r\\n  names.shift();\\r\\n\\r\\n  if (names.length > 1) {\\r\\n    for (const i of names) {\\r\\n      initials += i + \\\" \\\";\\r\\n    }\\r\\n  } */\\r\\n  names.splice(0,2)\\r\\n  console.log(names);\\r\\n\\r\\n  return (\\r\\n    <Container fluid className=\\\"main-content-container py-4 px-4\\\">\\r\\n      <Row className={\\\"mb-4\\\"}>\\r\\n        <Col>\\r\\n          <Card small className=\\\"mb-4 p-4\\\">\\r\\n            <h4>API Documentation</h4>\\r\\n            <span className=\\\"my-3\\\">\\r\\n              Master Facility List - External (MFL)\\r\\n              <br /> Esta documentao  referente aos recursos disponveis para\\r\\n              obter a lista actualizada das Unidades Sanitrias utilizadas pelo\\r\\n              Ministrio da Sade.\\r\\n            </span>\\r\\n\\r\\n            <div className=\\\"mt-3\\\">\\r\\n              <div>\\r\\n           