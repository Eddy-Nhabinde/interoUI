strito\\\",\\r\\n        id: \\\"district\\\",\\r\\n        options:options.district\\r\\n    },\\r\\n    {\\r\\n        title: \\\"Nome(Designao)\\\",\\r\\n        valueType: \\\"TEXT\\\",\\r\\n        id: \\\"name\\\",\\r\\n        placeholder: \\\"nome da unidade sanitria\\\",\\r\\n    },\\r\\n    {\\r\\n        title: \\\"Cdigo\\\",\\r\\n        valueType: \\\"TEXT\\\",\\r\\n        id: \\\"code\\\",\\r\\n        placeholder: \\\"cdigo da us\\\",\\r\\n    },\\r\\n    {\\r\\n        title: \\\"Classificao\\\",\\r\\n        valueType: \\\"LIST\\\",\\r\\n        id: \\\"classification\\\",\\r\\n        options:options.cfts,\\r\\n        placeholder: \\\"classificao\\\"\\r\\n    },\\r\\n]\\r\\n\\r\\nexport { variables }\",\"import React, { useEffect } from \\\"react\\\";\\r\\nimport {\\r\\n  HashRouter as Router,\\r\\n  Route,\\r\\n  Switch,\\r\\n  useHistory,\\r\\n  useLocation,\\r\\n  useRouteMatch,\\r\\n} from \\\"react-router-dom\\\";\\r\\nimport { routes } from \\\"../routes/routes\\\";\\r\\nimport withTracker from \\\"../../../withTracker\\\";\\r\\n\\r\\nconst MflInternalApp = () => {\\r\\n  const { path } = useRouteMatch();\\r\\n  const history = useHistory();\\r\\n  const location = useLocation()\\r\\n\\r\\n  useEffect(() => {\\r\\n    if (path === \\\"/mfl-internal\\\" && location.pathname.split(\\\"/\\\").length<3) {\\r\\n      history.replace(`/mfl-internal/mfl-internal-home`);\\r\\n    }\\r\\n  }, [path]);\\r\\n\\r\\n  return (\\r\\n    <Router>\\r\\n      <Switch>\\r\\n        {routes().map((route, index) => (\\r\\n          <Route\\r\\n            key={index}\\r\\n            path={route.path}\\r\\n            exact={route.exact}\\r\\n            component={withTracker((props) => {\\r\\n              return (\\r\\n                <route.layout {...props}>\\r\\n                  <route.component {...props} />\\r\\n                </route.layout>\\r\\n              );\\r\\n            })}\\r\\n          />\\r\\n        ))}\\r\\n      </Switch>\\r\\n    <